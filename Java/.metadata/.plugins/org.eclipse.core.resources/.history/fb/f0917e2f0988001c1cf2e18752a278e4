import java.util.Iterator;
import java.util.Objects;
import java.util.Random;

import org.w3c.dom.ranges.Range;

public class Password {

	 //Atributos
	private int longitud=8;
	private String contrasena;
	
	//Constructores
	public Password() {
		super();
	}
	public Password(int longitud) {
		super();
		this.longitud = longitud;
	}
	
	//MÃ©todos
	
	
	
	public String generarPassword() {
		String resultado="";
		int numero;
		char caracter;
		for (int i = 0; i < longitud; i++) {
			numero=(int) (Math.random()*123);
			while (numero<65 || numero==91 || numero==92 || numero==93 || numero==94 || numero==95 || numero==96) {
				numero=(int) (Math.random()*123);
			}
			caracter=(char) numero;
			resultado=resultado+caracter;
				
		}
		contrasena=resultado;
		return contrasena;
	}
	
	public boolean esFuerte() {
		boolean resultado=false;
		for (int i = 0; i < contrasena.length(); i++) {
			
		}
		
		return resultado;
	}
	
	public int getLongitud() {
		return longitud;
	}
	public String getContrasena() {
		return contrasena;
	}
	@Override
	public int hashCode() {
		return Objects.hash(contrasena);
	}
	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		Password other = (Password) obj;
		return Objects.equals(contrasena, other.contrasena);
	}
	
	
	
	
	
	
	
	
	
}
