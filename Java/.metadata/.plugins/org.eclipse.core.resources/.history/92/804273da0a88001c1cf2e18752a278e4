
public class Persona {

	//Atributos
	private String nombre;
	private int edad;
	private String DNI;
	private String password;
	private char sexo;
	private int peso;
	private double altura;
	public Persona() {
		super();
		this.nombre = nombre;
		this.edad = edad;
		DNI = dNI;
		this.password = new Password();
		this.sexo = sexo;
		this.peso = peso;
		this.altura = altura;
	}
	
	//Constructores
	
	
	
}
